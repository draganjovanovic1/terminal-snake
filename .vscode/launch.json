{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "terminal",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/build/terminal-snake/terminal-snake.dll",
            "args": [ "" ],
            "cwd": "${workspaceRoot}",
            "stopAtEntry": false,
            "externalConsole": true
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}